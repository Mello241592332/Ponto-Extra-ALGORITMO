Algoritmo "lista 1 - exercício 13"

Var
n1,n2,n3,m,s:inteiro


Inicio
 escreval("Digite um número inteiro: ")
 leia(n1)
 escreval("Digite o segundo: ")
 leia(n2)
 escreval("Digite o último inteiro: ")
 leia(n3)

  s <- n1+n3
  m <- s*n2

 escreva("Somando o primeiro com o último e multiplicando pelo segundo temos: ", m)


Fimalgoritmo