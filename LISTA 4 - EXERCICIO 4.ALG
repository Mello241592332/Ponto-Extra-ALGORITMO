algoritmo "LISTA 4 - EXERCICIO 5"

var
   a, b: real
inicio
   escreva("Digite o primeiro valor: ")
   leia(a)
   escreva("Digite o segundo valor: ")
   leia(b)
   se a > b entao
      escreva("Maior: ", a, " Menor: ", b)
   senao se a < b entao
      escreva("Maior: ", b, " Menor: ", a)
   senao
      escreva("Os valores são iguais.")
   fimse
fimalgoritmo


